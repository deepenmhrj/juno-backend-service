{
  "name": "juno_backend",
  "version": "1.0.1",
  "description": "Backend server for Juno",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node esbuild.config.js && node out.js",
    "start:watch": "nodemon",
    "start:tsc": "tsc && node dist/server.js",
    "start:watch:dev": "cp ./src/environments/.env.dev  .env && nodemon",
    "start:watch:dev:win": "copy .env .env.dev  && nodemon",
    "lint": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx,json,md,tsx,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,json,md,tsx,ts}\"",
    "lint-staged": "lint-staged --config lint-staged.config.mjs",
    "husky-install": "husky install"
  },
  "author": "@sgnilreutR",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^1.0.1",
    "@sentry/node": "^6.19.2",
    "@sentry/tracing": "^6.19.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "gaxios": "^4.3.2",
    "googleapis": "^100.0.0",
    "redis": "^4.0.6",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-loader": "^9.2.8",
    "typesync": "^0.9.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/express": "4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "17.0.23",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.13.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "tsc-files": "1.1.3",
    "typescript": "4.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
